#string methods

name = "petr"

print(len(name)) #lenght of string
print(name.find("e"))#find where the character is
print(name.capitalize())
print(name.upper())
print(name.isdigit())
print(name.isalpha())
print(name.count("e"))#how many charactes are in string
print(name.replace("e","o"))
print(name*3)#multiple string
